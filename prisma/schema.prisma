// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Event {
    id Int @id @default(autoincrement())
    type      String
    name      String
    complete  Boolean
}

model Participant {
    id Int @id @default(autoincrement())
    grade   Int
    name    String
    seen    Boolean
    dressed Boolean
}

model EventInformation {
    id Int @id @default(autoincrement())
    type           String
    name           String
    displayName    String
    complete       Boolean
    data           Participant[]
}