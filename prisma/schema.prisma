// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Rankings {
    id Int @id @default(autoincrement())
    grade      String
    points      String
}

model Participant {
    id Int @id @default(autoincrement())
    grade   Int
    name    String
    seen    Boolean
    dressed Boolean
}

model EventInformationDressUp {
    id Int @id @default(autoincrement())
    name           String
    displayName    String
    complete       Boolean
    data           Participant[]
    points         Int[]
}

model EventInformationStandard {
    id Int @id @default(autoincrement())
    type           String
    name           String
    displayName    String
    complete       Boolean
    rank           String[]
}